<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="smax.batch32.A2.controller.ProductList" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Product List">
		<content>
	 <List items="{/ProductSet}">
	            <items>
	               <StandardListItem title="{ProductID}" description="{Name}" type="Active"
	               press="gotoPage2"/>
	            </items>
	        </List>
		</content>
	</Page>
</core:View>